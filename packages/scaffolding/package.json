{
  "name": "@funish/scaffolding",
  "version": "0.0.4",
  "description": "Programmatically build scaffolding, powered by Funish.",
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "bin": {
    "scaffolding": "dist/cli.mjs"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prepack": "unbuild"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/funish/toolchains.git"
  },
  "keywords": [
    "scaffolding",
    "handlebars"
  ],
  "author": {
    "name": "Funish",
    "email": "official@funish.net",
    "url": "https://funish.net/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/funish/toolchains/issues"
  },
  "homepage": "https://github.com/funish/toolchains#readme",
  "dependencies": {
    "@funish/cli": "workspace:*",
    "@funish/prompt": "workspace:*",
    "c12": "1.10.0",
    "giget": "1.2.3",
    "handlebars": "4.7.8"
  }
}
